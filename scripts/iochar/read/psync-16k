[global]
include ../common.fio
ioengine=psync
bs=16k
stonewall

; Random Read

[randread-16k-1]
rw=randread
numjobs=1

[randread-16k-2]
rw=randread
numjobs=2

[randread-16k-4]
rw=randread
numjobs=4

[randread-16k-8]
rw=randread
numjobs=8

[randread-16k-16]
rw=randread
numjobs=16

; Sequential Read

[seqread-16k-1]
rw=read
numjobs=1

[seqread-16k-2]
rw=read
numjobs=2

[seqread-16k-4]
rw=read
numjobs=4

[seqread-16k-8]
rw=read
numjobs=8

[seqread-16k-16]
rw=read
