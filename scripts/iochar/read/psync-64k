[global]
include ../common.fio
ioengine=psync
bs=64k
stonewall

; Random Read

[randread-64k-1]
rw=randread
numjobs=1

[randread-64k-2]
rw=randread
numjobs=2

[randread-64k-4]
rw=randread
numjobs=4

[randread-64k-8]
rw=randread
numjobs=8

[randread-64k-16]
rw=randread
numjobs=16

; Sequential Read

[seqread-64k-1]
rw=read
numjobs=1

[seqread-64k-2]
rw=read
numjobs=2

[seqread-64k-4]
rw=read
numjobs=4

[seqread-64k-8]
rw=read
numjobs=8

[seqread-64k-16]
rw=read
numjobs=16
